
public abstract class AbstNode {
		  String data;
		  AbstNode next;
		  AbstNode prev;
		  AbstNode(String data, AbstNode next, AbstNode prev){
			   this.data = data;
			   this.next = next;
			   this.prev = prev;
			    }
		  public int size(){
			  return 0;
		  }
		  public AbstNode list(){
			  return this;
		  }
	}


