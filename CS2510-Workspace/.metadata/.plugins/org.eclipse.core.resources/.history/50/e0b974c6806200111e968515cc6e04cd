public class Head extends AbstNode{
	 String data;
	 AbstNode next;
	 AbstNode prev;
	  
	  Head(AbstNode next){
	   super("", next, null);
	    }
	  public int size(){
		  return 0  + next.size();
	  }
	  public String nextNodeData(){
		  return this.next.data;
	  }
	  public String dataList(){
		  return this.data.concat(this.next.dataList());
	  }
}
